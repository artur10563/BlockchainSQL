<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>

        <PackageId>BlockchainSQL</PackageId>
        <Version>1.0.6</Version>
        <Authors>artur10563</Authors>
        <Description>A library to intercept and log malicious SQL queries to the blockchain.</Description>
        <PackageTags>blockchain, sql, logging, proof_of_concept</PackageTags>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <RepositoryUrl>https://github.com/artur10563/BlockchainSQL</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.3.0" />
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.10" />
      <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="8.0.10" />
      <PackageReference Include="Microsoft.Extensions.Options" Version="10.0.0-preview.3.25171.5" />
      <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="8.0.0" />
      <PackageReference Include="Nethereum.Web3" Version="4.29.0" />
      <FrameworkReference Include="Microsoft.AspNetCore.App" />
      <None Include="README.md" Pack="true" PackagePath="/" />

    </ItemGroup>
    

</Project>
